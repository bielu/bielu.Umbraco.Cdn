{
  "$schema": "./umbraco/config/appsettings-schema.json",
  "Serilog": {
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Warning",
        "Microsoft.Hosting.Lifetime": "Information",
        "System": "Warning"
      }
    }
  },
  "ConnectionStrings": {
    "umbracoDbDSN": "Data Source=|DataDirectory|/Umbraco.sqlite.db;Cache=Shared;Foreign Keys=True;Pooling=True",
    "umbracoDbDSN_ProviderName": "Microsoft.Data.SQLite"
  },
  "Bielu": {
    "Cdn": {
      "DevMode": false,
      "cloudflare": {
        "enabled": true,
        "token": "-- token - use secret.json --"
      },
      "Azure": {
        "FrontDoor": {
          "Disabled": true
        }
      },
      "Akamai": {
        "Disabled": true
      },
      "AWS": {
        "CloudFront": {
          "Disabled": true
        }
      }
    }
  },
  "Umbraco": {
    "CMS": {
      "TypeFinder": {
        "AdditionalAssemblyExclusionEntries": [
          "dotnet-nswag",
          "NJsonSchema.CodeGeneration.CSharp",
          "NJsonSchema.CodeGeneration",
          "NSwag.CodeGeneration.CSharp",
          "NSwag.CodeGeneration.TypeScript",
          "NSwag.Generation.WebApi"
        ]
      },
      "Global": {
        "Id": "25317305-212c-4ae3-ae1e-57385ed5df44",
        "SanitizeTinyMce": true
      },
      "Unattended": {
        "InstallUnattended": true,
        "PackageMigrationsUnattended": true,
        "UpgradeUnattended": true,
        "UnattendedUserName": "bielu",
        "UnattendedUserEmail": "bielu@bielu.pl",
        "UnattendedUserPassword": "Password1!"
      },
      "Content": {
        "ContentVersionCleanupPolicy": {
          "EnableCleanup": true
        }
      }
    }
  }
}